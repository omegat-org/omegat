        </rule>
        <rule break="yes">
          <beforebreak>\!\"</beforebreak>
          <afterbreak>\s</afterbreak>
        </rule>
      </languagerule>

      <!-- ZH
      Chinese non-break rules
      http://sourceforge.net/p/omegat/feature-requests/945/ -->

      <languagerule languagerulename="Chinese">
        <rule break="no">
          <beforebreak>[。？！—]</beforebreak>
          <afterbreak>[’”"」）』？—]</afterbreak>
        </rule>
        <rule break="no">
          <beforebreak>[、,.．]</beforebreak>
          <afterbreak>.</afterbreak>
        </rule>

      <!-- ZH
      Chinese break rules -->

        <rule break="yes">
          <beforebreak>[。？！]</beforebreak>
          <afterbreak>.</afterbreak>
        </rule>
      </languagerule>

      <!-- Default language-related segmentation rules -->

      <languagerule languagerulename="Default">
        <rule break="no">
          <!-- ... exception -->
          <beforebreak>\.\.\.</beforebreak>
          <afterbreak>\s+\P{Lu}</afterbreak>
        </rule>
        <rule break="yes">
          <!--numbered headers break rule 
         based on an idea by Jean-Christophe Helary -->
          <beforebreak>^\s*\p{Nd}+[\p{Nd}\.\)\]]+</beforebreak>
          <afterbreak>\s+\p{Lu}</afterbreak>
        </rule>
        <rule break="yes">
          <!-- .?! break rule -->
          <beforebreak>[\.\?\!]+</beforebreak>
          <afterbreak>\s</afterbreak>
        </rule>
      </languagerule>

      <!-- Text files format-related rule -->

      <languagerule languagerulename="Text">
        <!-- special handling for Text files to break on empty indented lines
     idea by Jean-Christophe Helary -->

        <rule break="yes">
          <beforebreak>\n</beforebreak>
          <afterbreak> +</afterbreak>
        </rule>
      </languagerule>

      <!-- HTML format-related rule -->

      <languagerule languagerulename="HTML">
        <!-- special handling for BR tag to segment on it
     idea by Jean-Christophe Helary -->

        <rule break="yes">
          <beforebreak>&lt;br\d+/?&gt;</beforebreak>
          <afterbreak>.</afterbreak>
        </rule>
      </languagerule>
    </languagerules>

    <maprules>
      <languagemap languagepattern="CA.*" languagerulename="Catalan"/>
      <languagemap languagepattern="CS.*" languagerulename="Czech"/>
      <languagemap languagepattern="DE.*" languagerulename="German"/>
      <languagemap languagepattern="EN.*" languagerulename="English"/>
      <languagemap languagepattern="ES.*" languagerulename="Spanish"/>
      <languagemap languagepattern="FI.*" languagerulename="Finnish"/>
      <languagemap languagepattern="FR.*" languagerulename="French"/>
      <languagemap languagepattern="IT.*" languagerulename="Italian"/>
      <languagemap languagepattern="JA.*" languagerulename="Japanese"/>
      <languagemap languagepattern="NL.*" languagerulename="Dutch"/>
      <languagemap languagepattern="PL.*" languagerulename="Polish"/>
      <languagemap languagepattern="RU.*" languagerulename="Russian"/>
      <languagemap languagepattern="SV.*" languagerulename="Swedish"/>
      <languagemap languagepattern="SK.*" languagerulename="Slovak"/>
      <languagemap languagepattern="ZH.*" languagerulename="Chinese"/>
      <languagemap languagepattern=".*" languagerulename="Default"/>
      <languagemap languagepattern=".*" languagerulename="Text"/>
      <languagemap languagepattern=".*" languagerulename="HTML"/>
    </maprules>
  </body>
