<?xml version="1.0"?>
<DockingDesktop version="2.0">
  <!--
	  orientation="1" : vertical
	  orientation="0" : horizontal
	  location="0.n" : width of the left (or top) part
	  only 2 items per "split"
  -->

  <DockingPanel>
	<Split orientation="1" location="0.25">
	  <Split orientation="0" location="0.20">
		<Dockable>
		  <Key dockName="NOTES" />
		</Dockable>
		<Dockable>
		  <Key dockName="GLOSSARY" />
		</Dockable>
	  </Split>
	  <Split orientation="1" location="0.65">
		<Split orientation="0" location="0.75">
		  <Dockable>
			<Key dockName="EDITOR" />
		  </Dockable>
		<Split orientation="1" location="0.5">
		  <Dockable>
			<Key dockName="SEGMENTPROPERTIES" />
		  </Dockable>
		  <Dockable>
			<Key dockName="COMMENTS" />
		  </Dockable>
		</Split>
		</Split>
		<Split orientation="0" location="0.8">
		  <Dockable>
			<Key dockName="MATCHES" />
		  </Dockable>
		  <Dockable>
			<Key dockName="MULTIPLE_TRANS" />
		  </Dockable>
		</Split>
	  </Split>
	</Split>
  </DockingPanel>
  

  <Border zone="2">
	<Dockable>
	  <Key dockName="DICTIONARY" />
	  <RelativePosition x="0.6" y="0.7" w="0.4" h="0.3" />
	</Dockable>
	<Dockable>
	  <Key dockName="MACHINE_TRANSLATE" />
	  <RelativePosition x="0.6" y="0.7" w="0.4" h="0.3" />
	</Dockable>
  </Border>

  <TabGroups></TabGroups>
</DockingDesktop>
